<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <mfgoogletagmanager>
            <general>
                <enabled>0</enabled>
                <mfmodule>GoogleTagManager</mfmodule>
                <mftype>1</mftype>
                <install_gtm>use_public_id</install_gtm>
            </general>
            <web_container>
                <script_content/>
                <noscript_content/>
                <enable>1</enable>
                <account_id/>
                <container_id/>
                <public_id/>
            </web_container>
            <analytics>
                <enable>1</enable>
                <measurement_id/>
            </analytics>
            <attributes>
                <product>sku</product>
                <brand/>
                <categories/>
            </attributes>
            <customer_data>
                <protect>0</protect>
            </customer_data>
        </mfgoogletagmanager>
    </default>
</config>
